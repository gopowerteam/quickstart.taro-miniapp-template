{
    "name": "taro-react-weapp",
    "version": "1.0.0",
    "private": true,
    "description": "React-For-Taro-Weapp",
    "templateInfo": {
        "name": "taro-ui",
        "typescript": true,
        "css": "stylus"
    },
    "scripts": {
        "build:weapp": "cross-env NODE_ENV=production taro build --type weapp",
        "build:swan": "cross-env NODE_ENV=production taro build --type swan",
        "build:alipay": "cross-env NODE_ENV=production taro build --type alipay",
        "build:tt": "cross-env NODE_ENV=production taro build --type tt",
        "build:h5": "cross-env NODE_ENV=production taro build --type h5",
        "build:rn": "cross-env NODE_ENV=production taro build --type rn",
        "build:qq": "cross-env NODE_ENV=production taro build --type qq",
        "build:jd": "cross-env NODE_ENV=production taro build --type jd",
        "build:dd": "cross-env NODE_ENV=production taro build --type dd",
        "build:quickapp": "cross-env NODE_ENV=production taro build --type quickapp",
        "dev:weapp": "cross-env NODE_ENV=development npm run build:weapp -- --watch",
        "dev:swan": "cross-env NODE_ENV=development npm run build:swan -- --watch",
        "dev:alipay": "cross-env NODE_ENV=development npm run build:alipay -- --watch",
        "dev:tt": "cross-env NODE_ENV=development npm run build:tt -- --watch",
        "dev:h5": "cross-env NODE_ENV=development npm run build:h5 -- --watch",
        "dev:rn": "cross-env NODE_ENV=development npm run build:rn -- --watch",
        "dev:qq": "cross-env NODE_ENV=development npm run build:qq -- --watch",
        "dev:jd": "cross-env NODE_ENV=development npm run build:jd -- --watch",
        "dev:dd": "cross-env NODE_ENV=development taro build --type dd --watch",
        "dev:quickapp": "npm run build:quickapp -- --watch",
        "request-generate": "http-request-cli -g"
    },
    "browserslist": [
        "last 3 versions",
        "Android >= 4.1",
        "ios >= 8"
    ],
    "author": "",
    "dependencies": {
        "@babel/runtime": "^7.14.0",
        "@gopowerteam/http-request": "^0.2.2",
        "@tarojs/cli": "3.2.8",
        "@tarojs/components": "3.2.8",
        "@tarojs/react": "3.2.8",
        "@tarojs/runtime": "3.2.8",
        "@tarojs/taro": "3.2.8",
        "ahooks": "^2.10.4",
        "axios-miniprogram-adapter": "^0.3.2",
        "cos-js-sdk-v5": "^1.2.10",
        "lodash": "^4.17.21",
        "react": "^17.0.2",
        "react-dom": "^17.0.0",
        "reto": "^0.9.3",
        "rxjs": "^7.0.1",
        "taro-ui": "^3.0.0-alpha.10",
        "tarojs-router-next": "^2.2.2",
        "tarojs-router-next-plugin": "^2.2.2"
    },
    "devDependencies": {
        "@babel/core": "^7.14.2",
        "@gopowerteam/http-request-cli": "^0.2.0",
        "@tarojs/mini-runner": "3.2.8",
        "@tarojs/webpack-runner": "3.2.8",
        "@types/react": "^17.0.5",
        "@types/webpack-env": "^1.16.0",
        "@typescript-eslint/eslint-plugin": "4.23.0",
        "@typescript-eslint/parser": "4.23.0",
        "babel-preset-taro": "3.2.8",
        "cross-env": "^7.0.3",
        "eslint": "7.26.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-config-taro": "3.2.8",
        "eslint-plugin-import": "2.23.2",
        "eslint-plugin-prettier": "^3.4.0",
        "eslint-plugin-react": "7.8.2",
        "eslint-plugin-react-hooks": "4.2.0",
        "prettier": "^2.3.2",
        "promise-prototype-finally": "^1.0.0",
        "stylelint": "^13.13.1",
        "taro-plugin-tailwind": "^1.1.10",
        "typescript": "^4.2.4"
    }
}
